{
    "name": "atournayre/domain-component",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "atournayre",
            "email": "aurelien.tournayre@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.3",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^1.7",
        "doctrine/orm": "^2.9",
        "symfony/validator": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Atournayre\\Component\\Domain\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Atournayre\\Component\\Domain\\Tests\\": ""
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3",
        "symfony/dotenv": "^5.3"
    }
}
